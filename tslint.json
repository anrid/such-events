{
  "rules": {
    "indent": [true, "spaces", 2],
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-unused-variable": true,
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [true, "never"],
    "eofline": true
  }
}